le.Height - lb_message.Height)&#xD;&#xA;&#x9;&#x9;.Location = New System.Drawing.Point(lb_message.Right - .Width, lb_message.Bottom)&#xD;&#xA;&#x9;&#x9;.TextAlign = ContentAlignment.MiddleCenter&#xD;&#xA;&#x9;&#x9;.Font = New Font(fontName, fontSize - 2, FontStyle.Regular)&#xD;&#xA;&#x9;&#x9;.Padding = New Padding(0,0,0,0)&#xD;&#xA;&#x9;&#x9;'.BorderStyle = BorderStyle.FixedSingle '// for debug&#xD;&#xA;&#x9;End With&#xD;&#xA;&#x9;panel.Controls.Add(lb_dtime)&#xD;&#xA;End If&#xD;&#xA;lb_dtime.Text = DateTime.Now.ToString(&quot;HH:mm:ss&quot;)&#xD;&#xA;&#xD;&#xA;'// ProgressLabel&#xD;&#xA;Dim lb_pgs As Label&#xD;&#xA;If Not(panel.Controls(&quot;ProgressLabel&quot;) Is Nothing) Then&#xD;&#xA;&#x9;lb_pgs = CType(panel.Controls(&quot;ProgressLabel&quot;), Label)&#xD;&#xA;Else&#xD;&#xA;&#x9;lb_pgs = New Label() &#xD;&#xA;&#x9;With lb_pgs&#xD;&#xA;&#x9;&#x9;.Name = &quot;ProgressLabel&quot;&#xD;&#xA;&#x9;&#x9;.Size = New Size(CInt(panel.Width * 0.15), panel.Height - lb_title.Height - lb_message.Height)&#xD;&#xA;&#x9;&#x9;.Location = New System.Drawing.Point(lb_dtime.Left -lb_pgs.Width , lb_dtime.top)&#xD;&#xA;&#x9;&#x9;.Font = New Font(fontName, fontSize - 2, FontStyle.Regular)&#xD;&#xA;&#x9;&#x9;.TextAlign = ContentAlignment.MiddleCenter&#xD;&#xA;&#x9;&#x9;'.BorderStyle = BorderStyle.FixedSingle '// for debug&#xD;&#xA;&#x9;End With&#xD;&#xA;&#x9;panel.Controls.Add(lb_pgs)&#xD;&#xA;End If&#xD;&#xA;lb_pgs.Text = progressRate &amp; &quot;%&quot;&#xD;&#xA;lb_pgs.Visible = displayProgress&#xD;&#xA;&#xD;&#xA;'// ProgressBar&#xD;&#xA;Dim pgsBar As ProgressBar&#xD;&#xA;If Not(panel.Controls(&quot;ProgressBar&quot;) Is Nothing) Then&#xD;&#xA;&#x9;pgsBar = CType(panel.Controls(&quot;ProgressBar&quot;), ProgressBar)&#xD;&#xA;Else&#xD;&#xA;&#x9;pgsBar = New ProgressBar()&#xD;&#xA;&#x9;With pgsBar&#xD;&#xA;&#x9;&#x9;.Name = &quot;ProgressBar&quot;&#xD;&#xA;&#x9;&#x9;.Size = New Size(CInt(panel.Width * 0.65), CInt(lb_dtime.Height * 0.4))&#xD;&#xA;&#x9;&#x9;.Location = New System.Drawing.Point(lb_message.Left, lb_message.Bottom + CInt(lb_dtime.Height * 0.4))&#xD;&#xA;&#x9;&#x9;.Minimum = 0&#xD;&#xA;&#x9;&#x9;.Maximum = 100&#xD;&#xA;&#x9;End With&#xD;&#xA;&#x9;panel.Controls.Add(pgsBar)&#xD;&#xA;End If&#xD;&#xA;pgsBar.Value = If(progressRate &gt; 100, 100, If(progressRate &lt; 0, 0, progressRate))&#xD;&#xA;pgsBar.Visible = displayProgress&#xD;&#xA;&#xD;&#xA;'// Update&#xD;&#xA;panel.Update()&#xD;&#xA;form.Update()&#xD;&#xA;form.Refresh()&#xD;&#xA;&#xD;&#xA;'// Close&#xD;&#xA;If formClose Then&#xD;&#xA;&#x9;form.Close()&#xD;&#xA;&#x9;form.Dispose()&#xD;&#xA;End If" DisplayName="コードを呼び出し - フォームを表示" sap:VirtualizedContainerService.HintSize="334,164" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
