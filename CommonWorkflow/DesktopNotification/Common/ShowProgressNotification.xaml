    <ui:InvokeCode ContinueOnError="{x:Null}" Code="Const fontName As String = &quot;Meiryo UI&quot;&#xD;&#xA;Const fontSize As Int32 = 10&#xD;&#xA;Const formName As String = &quot;UiPath-DesktopNotification&quot;&#xD;&#xA;'// ColorTheme：Blue&#xD;&#xA;Dim formBackColor As Color = Color.FromArgb(25,118,210)&#xD;&#xA;Dim formForeColor As Color  = Color.White&#xD;&#xA;'// ColorTheme：Black&#xD;&#xA;'Dim formBackColor As Color = Color.Black&#xD;&#xA;'Dim formForeColor As Color  = Color.White&#xD;&#xA;'// ColorTheme：White&#xD;&#xA;'Dim formBackColor As Color = Color.White&#xD;&#xA;'Dim formForeColor As Color  = Color.Black&#xD;&#xA;'// ColorTheme：Gray&#xD;&#xA;'Dim formBackColor As Color = Color.Gray&#xD;&#xA;'Dim formForeColor As Color  = Color.Black&#xD;&#xA;&#xD;&#xA;'// Form&#xD;&#xA;Dim form As Form&#xD;&#xA;Dim openForm As Boolean = False&#xD;&#xA;If Application.OpenForms.Count &gt; 0 Then&#xD;&#xA;&#x9;'// Object Reuse&#xD;&#xA;&#x9;For i As Integer = 0 To Application.OpenForms.Count -1&#xD;&#xA;&#x9;&#x9;If Application.OpenForms(i).Name.ToString = formName Then&#xD;&#xA;&#x9;&#x9;&#x9;form = Application.OpenForms(i)&#xD;&#xA;&#x9;&#x9;&#x9;Exit For&#xD;&#xA;&#x9;&#x9;End If&#xD;&#xA;&#x9;Next&#xD;&#xA;End If&#xD;&#xA;If form Is Nothing Then&#xD;&#xA;&#x9;openForm = True&#xD;&#xA;&#x9;form = New Form()&#xD;&#xA;&#x9;With form&#xD;&#xA;&#x9;&#x9;.Name = formName&#xD;&#xA;&#x9;&#x9;.StartPosition = FormStartPosition.Manual&#xD;&#xA;&#x9;&#x9;.Width  = CInt(Screen.PrimaryScreen.WorkingArea.Width * 0.22)&#xD;&#xA;&#x9;&#x9;.Height = CInt(Screen.PrimaryScreen.WorkingArea.Height * 0.12)&#xD;&#xA;&#x9;&#x9;.Left = Screen.PrimaryScreen.WorkingArea.Width - .Width - CInt(.Height * 0.1)&#xD;&#xA;&#x9;&#x9;.Top  = Screen.PrimaryScreen.WorkingArea.Height - .Height - CInt(.Height * 0.1)&#xD;&#xA;&#x9;&#x9;.Font = New Font(fontName, fontSize, FontStyle.Regular)&#xD;&#xA;&#x9;&#x9;.BackColor = formBackColor&#xD;&#xA;&#x9;&#x9;.ForeColor = formForeColor&#xD;&#xA;&#x9;&#x9;.FormBorderStyle = FormBorderStyle.None&#xD;&#xA;&#x9;&#x9;.Opacity = 1.0&#xD;&#xA;&#x9;&#x9;.Padding = New Padding(CInt(.Height * 0.06))&#xD;&#xA;&#x9;End With&#xD;&#xA;&#x9;form.Show()&#xD;&#xA;End If&#xD;&#xA;'form.BringToFront()&#xD;&#xA;form.TopMost = True&#xD;&#xA;form.TopMost = False&#xD;&#xA;&#xD;&#xA;'// Panel&#xD;&#xA;Dim panel As Panel&#xD;&#xA;If Not(form.Controls(&quot;Panel&quot;) Is Nothing) Then&#xD;&#xA;&#x9;panel = CType(form.Controls(&quot;Panel&quot;), Panel)&#xD;&#xA;Else&#xD;&#xA;&#x9;panel = New Panel()&#xD;&#xA;&#x9;With panel&#xD;&#xA;&#x9;&#x9;.Name = &quot;Panel&quot;&#xD;&#xA;&#x9;&#x9;.Width = CInt(form.Width - form.Padding.Left - form.Padding.Right)&#xD;&#xA;&#x9;&#x9;.Height = CInt(form.Height - form.Padding.Top - form.Padding.Bottom)&#xD;&#xA;&#x9;&#x9;.Location = New Point(form.Padding.Right, form.Padding.Bottom)&#xD;&#xA;&#x9;&#x9;'.panel.BackColor = Color.Beige '// for debug&#xD;&#xA;&#x9;End With&#xD;&#xA;&#x9;form.Controls.Add(panel)&#xD;&#xA;End If&#xD;&#xA;&#xD;&#xA;'// Title&#xD;&#xA;Dim lb_title As Label&#xD;&#xA;If Not(panel.Controls(&quot;Title&quot;) Is Nothing) Then&#xD;&#xA;&#x9;lb_title = CType(panel.Controls(&quot;Title&quot;), Label)&#xD;&#xA;Else&#xD;&#xA;&#x9;lb_title = New Label()&#xD;&#xA;&#x9;With lb_title&#xD;&#xA;&#x9;&#x9;.Name = &quot;Title&quot;&#xD;&#xA;&#x9;&#x9;.Size = New Size(CInt(panel.Width* 0.92), CInt(panel.Height * 0.22))&#xD;&#xA;&#x9;&#x9;.Location = New System.Drawing.Point(0, 0)&#xD;&#xA;&#x9;&#x9;.Font = New Font(fontName, fontSize, FontStyle.Bold)&#xD;&#xA;&#x9;&#x9;.TextAlign = ContentAlignment.MiddleLeft&#xD;&#xA;&#x9;&#x9;.Padding = New Padding(0,0,0,0)&#xD;&#xA;&#x9;&#x9;'.BorderStyle = BorderStyle.FixedSingle '// for debug&#xD;&#xA;&#x9;End With&#xD;&#xA;&#x9;panel.Controls.Add(lb_title)&#xD;&#xA;End If&#xD;&#xA;lb_title.Text = title&#xD;&#xA;&#xD;&#xA;'// Close&#xD;&#xA;If panel.Controls(&quot;Close&quot;) Is Nothing Then&#xD;&#xA;&#x9;Dim btn_close As Button = New Button() &#xD;&#xA;&#x9;With b